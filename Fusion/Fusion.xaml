<Window x:Class="Fusion.FusionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FusionWindow" Height="600" Width="800">
    <Grid Background="Black">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <WebBrowser Name="Browser" Grid.Row="0" Grid.Column="0" Source="http://intranet/" Navigated="Browser_Navigated" LoadCompleted="Browser_LoadCompleted">
        </WebBrowser>
    </Grid>
</Window>
